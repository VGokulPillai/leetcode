# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def mergeInBetween(self, list1: ListNode, a: int, b: int, list2: ListNode) -> ListNode:
        arr1=[]
        curr=list1
        while(curr):
            arr1.append(curr.val)
            curr=curr.next
        
        arr2=[]
        curr2=list2
        while(curr2):
            arr2.append(curr2.val)
            curr2=curr2.next
            
        arr3=arr1[:a]+arr2+arr1[b+1:]
        
        head=ListNode(arr3[0])
        curr=head
        for i in arr3[1:]:
            curr.next=ListNode(i)
            curr=curr.next
        return head
